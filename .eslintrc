{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },

  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-no-undef": 1,
    "react/jsx-equals-spacing": [1, "always"],
    "react/prop-types": [1, {"ignore": ["children"]} ],
    "arrow-parens": [2, "as-needed"],
    //"camelcase": 2,
    "semi": [2, "never"],
    "strict": 0
  }
}
